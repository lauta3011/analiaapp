{
  "expo": {
    "name": "GLAM",
    "slug": "analiaapp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/glam.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/glam.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"    
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/glam.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lauta3011.analiaapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/glam.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "355d34f8-d015-4867-a5ce-569714a30802"
      }
    }
  }
}
